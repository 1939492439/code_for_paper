var median_ndvi=[]
var median_ndpi=[]
var median_ndgi=[]

GetMedianValue();
Draw();
 
 print( median_ndvi,"SG_median_Value_NDVI")
 print( median_ndpi,"SG_median_Value_NDPI")
 print( median_ndgi,"SG_median_Value_NDGI")
function GetMedianValue(){
  
  
var temp

temp=ee.ImageCollection(sg_ndvi_image.select("sg_ndvi")).getRegion(xiaomai_2,10).slice(1,32).map(function(item){return ee.List(item).get(4)})
 median_ndvi.push(ee.List(temp).reduce(ee.Reducer.mean()))
 temp=ee.ImageCollection(sg_ndpi_image.select("sg_ndpi")).getRegion(xiaomai_2,10).slice(1,32).map(function(item){return ee.List(item).get(4)})
 median_ndpi.push(ee.List(temp).reduce(ee.Reducer.mean()))
 temp=ee.ImageCollection(sg_ndgi_image.select("sg_ndgi")).getRegion(xiaomai_2,10).slice(1,32).map(function(item){return ee.List(item).get(4)})
 median_ndgi.push(ee.List(temp).reduce(ee.Reducer.mean()))
 

for(var i=1;i<30;i++){
  temp=ee.ImageCollection(sg_ndvi_image.select("sg_ndvi_"+i)).getRegion(xiaomai_2,10).slice(1,32).map(function(item){return ee.List(item).get(4)})
 median_ndvi.push(ee.List(temp).reduce(ee.Reducer.mean()))
 temp=ee.ImageCollection(sg_ndpi_image.select("sg_ndpi_"+i)).getRegion(xiaomai_2,10).slice(1,32).map(function(item){return ee.List(item).get(4)})
 median_ndpi.push(ee.List(temp).reduce(ee.Reducer.mean()))
 temp=ee.ImageCollection(sg_ndgi_image.select("sg_ndgi_"+i)).getRegion(xiaomai_2,10).slice(1,32).map(function(item){return ee.List(item).get(4)})
 median_ndgi.push(ee.List(temp).reduce(ee.Reducer.mean()))
  
}
  
}

function   Draw(){
  
 var vi=ui.Chart.array.values(ee.List(median_ndvi), 0)  
                .setSeriesNames(["中值参考曲线"])  
                .setOptions({  
                  title: "median_ndvi",   
                  hAxis: {title: "time"},  
                  vAxis: {title: "value"},  
                  lineWidth:1,  
                  pointSize:2  
                });  
                
 var pi=ui.Chart.array.values(ee.List(median_ndpi), 0)  
                .setSeriesNames(["中值参考曲线"])  
                .setOptions({  
                  title: "median_ndpi",   
                  hAxis: {title: "time"},  
                  vAxis: {title: "value"},  
                  lineWidth:1,  
                  pointSize:2  
                });                  
 var gi=ui.Chart.array.values(ee.List(median_ndgi), 0)  
                .setSeriesNames(["中值参考曲线"])  
                .setOptions({  
                  title: "median_ndgi",   
                  hAxis: {title: "time"},  
                  vAxis: {title: "value"},  
                  lineWidth:1,  
                  pointSize:2  
                });                
                
                
print(vi)
print(pi)
print(gi)
  
}

 


